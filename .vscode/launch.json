{
    "configurations": [
        {
            "name": "Python Debugger: Run CLI",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/main.py",
            "console": "integratedTerminal",
            "args": [
                "generate", "--profile", ".ignore/profile_template.json", "--search-term", "senior software engineer", "-l", "Philadelphia, PA"
            ],
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}
